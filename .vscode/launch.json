{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Remote Attach",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "."
        }
      ]
    },
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "${command:pickArgs}"
    },
    {
      "name": "Split dataset",
      "type": "debugpy",
      "request": "launch",
      "program": "/home/sberti/metaCub/split_lerobot_dataset_by_action.py",
      "console": "integratedTerminal",
      "python": "/home/sberti/.local/share/mamba/envs/lerobot/bin/python",
      "cwd": "/home/sberti/metaCub"
    },
    {
      "name": "Test (Real)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/lerobot/scripts/lerobot_record.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "preLaunchTask": "clear-test",
      "args": [
        "--robot.type=ergocub",
        "--robot.remote_prefix=/ergocubSim",
        "--robot.local_prefix=/lerobot",
        "--robot.cameras={ external: {\"type\": \"yarp\", \"yarp_name\": \"camera\", \"width\": 640, \"height\": 480, \"fps\": 30}, egocentric:{\"type\": \"yarp\", \"yarp_name\": \"ergocubSim/depthCamera/rgbImage:o\", \"width\": 640, \"height\": 480, \"fps\": 30}}",
        "--robot.control_boards=[head,left_hand,right_hand,fingers]",
        "--dataset.repo_id=steb6/eval_redballppp",
        "--dataset.fps=10",
        "--policy.path=steb6/train-redball-act",
        "--dataset.single_task=get the ball",
        "--dataset.reset_time=0",
        "--dataset.episode_time=10",
        "--dataset.fps=10"
      ]
    },
    {
      "name": "Test (Sim)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/lerobot/scripts/lerobot_record.py",
      "console": "integratedTerminal",
      "python": "/home/sberti/.local/share/mamba/envs/lerobot/bin/python",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "preLaunchTask": "clear-test",
      "args": [
        "--robot.type=ergocub",
        "--robot.remote_prefix=/ergocubSim",
        "--robot.local_prefix=/lerobot",
        "--robot.cameras={egocentric:{\"type\": \"yarp\", \"yarp_name\": \"ergocubSim/depthCamera/rgbImage:o\", \"width\": 640, \"height\": 480, \"fps\": 30}}",
        "--robot.control_boards=[head,bimanual,fingers]",
        "--dataset.repo_id=steb6/eval_test",
        "--policy.path=steb6/act_HOME_wave_shake-videomaefeat-nocam",
        "--dataset.single_task=interact with the human",
        "--dataset.reset_time=0",
        "--dataset.episode_time=300",
        "--compute_videomae_features_live=true"
      ]
    },
    {
      "name": "Train",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/lerobot/scripts/lerobot_train.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "preLaunchTask": "clear-train",
      "python": "/home/sberti/.local/share/mamba/envs/lerobot/bin/python",
      "args": [
        "--policy.type=act",
        "--policy.repo_id=HSP-IIT/test",
        "--dataset.repo_id=steb6/redball_sim_reactive",
        "--policy.device=cuda",
        "--output_dir=checkpoints/test"
      ]
    },
    {
      "name": "Record (Sim)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/lerobot/scripts/lerobot_record.py",
      "console": "integratedTerminal",
      "python": "/home/sberti/.local/share/mamba/envs/lerobot/bin/python",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "preLaunchTask": "clear-record",
      "args": [
        "--robot.type=ergocub",
        "--robot.remote_prefix=/ergocubSim",
        "--robot.local_prefix=/lerobot",
        "--robot.cameras={egocentric:{\"type\": \"yarp\", \"yarp_name\": \"stream/video:o\", \"width\": 640, \"height\": 480, \"fps\": 30}}",
        "--robot.control_boards=[head,bimanual]",
        "--teleop.finger_scale=1",
        "--teleop.type=metaquest",
        "--dataset.repo_id=steb6/HOME_GZ_shake",
        "--dataset.num_episodes=1",
        "--dataset.fps=10",
        "--dataset.reset_time=1",
        "--dataset.episode_time=280",
        "--dataset.single_task=interact with the human",
        "--compute_videomae_features_live=false"
      ]
    },
    {
      "name": "Visualize dataset",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/lerobot/scripts/lerobot_dataset_viz.py",
      "console": "integratedTerminal",
      "python": "/home/sberti/.local/share/mamba/envs/lerobot/bin/python",
      "cwd": "/home/sberti/metaCub",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--repo-id=steb6/hri_real_3"
      ],
      "justMyCode": false
    },
    {
      "name": "Record Real",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/lerobot/scripts/lerobot_record.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      // "preLaunchTask": "clear-record",
      "args": [
        "--robot.type=ergocub",
        "--robot.remote_prefix=/ergocub",
        "--robot.local_prefix=/lerobot",
        "--robot.cameras={ external: {\"type\": \"yarp\", \"yarp_name\": \"depthCamera/rgbImage:o\", \"width\": 640, \"height\": 480, \"fps\": 30}}",
        "--robot.control_boards=[head,left_hand,right_hand,fingers,torso]",
        "--teleop.type=metaquest",
        "--dataset.repo_id=steb6/hri_real_3",
        "--dataset.num_episodes=1",
        "--dataset.fps=10",
        "--dataset.reset_time=3",
        "--dataset.episode_time=300",
        "--dataset.single_task=interact with the human",
        "--resume=true"
      ]
    }
  ]
}